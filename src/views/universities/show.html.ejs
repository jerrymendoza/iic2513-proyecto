<%- include('../menu', {active: 'universities'}) %>
<h3><%= university.name %></h3>
<ul>
    <li>Code: <%= university.code %></li>
    <li>Domain: <%= university.domain %></li>
</ul>

<div class="card-container">
    <% university.Courses.forEach((course) => {%>
        <%- include('../courses/_show', {course}) %>
    <% }) %>
    <div class="card new">
      <a href=<%= newCoursePath %>>New Course</a>
    </div>
  </div>
